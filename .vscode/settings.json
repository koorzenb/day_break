{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.dart_tool/": true,
    "**/.idea/": true,
    "**/.metadata": true,
    "**/build/": true,
    "**/*.g.dart": false,
    "**/*.freezed.dart": false,
    "pubspec.lock": true,
    "linux/": true,
    "windows/": true,
    "macos/": true,
    "web/": true
  },
  "search.exclude": {
    "**/build/": true,
    "**/.dart_tool/": true,
    "**/*.lock": true,
    "**/ios/Pods/": true,
    "**/ios/Flutter/App.framework": true,
    "**/android/app/build/": true,
    "**/linux/": true,
    "**/windows/": true,
    "**/macos/": true,
    "**/web/": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/build/**": true,
    "**/.dart_tool/**": true,
    "**/ios/Pods/**": true,
    "**/android/app/build/**": true
  },
  "dart.flutterSdkPath": ".fvm/versions/3.35.3",
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.rulers": [
      80
    ]
  }
}